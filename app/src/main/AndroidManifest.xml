<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.iiseeuu.dagger2demo"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:name=".scope.App"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity2" android:label="Login"/>
        <activity android:name=".scope.a.AActivity" android:label="A"/>
        <activity android:name=".scope.b.BActivity" android:label="B"/>
        <activity android:name=".scope.c.CActivity" android:label="C"/>
    </application>

</manifest>